#rotary-table {
  width: 340px;
  height: 349px;
  position: relative;
  margin: auto;
  background-color: antiquewhite;
}
#rotary-table .award {
  width: 90px;
  height: 96px;
  line-height: 96px;
  text-align: center;
  float: left;
  position: absolute;
  overflow: hidden;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
}
#rotary-table .award.active {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
}
#rotary-table .award.award0 {
  top: 21px;
  left: 21px;
}
#rotary-table .award.award1 {
  top: 21px;
  left: 125px;
}
#rotary-table .award.award2 {
  top: 21px;
  right: 22px;
}
#rotary-table .award.award3 {
  top: 126px;
  right: 22px;
}
#rotary-table .award.award4 {
  bottom: 22.5px;
  right: 22px;
}
#rotary-table .award.award5 {
  bottom: 22.5px;
  right: 125.5px;
}
#rotary-table .award.award6 {
  bottom: 22.5px;
  left: 21px;
}
#rotary-table .award.award7 {
  top: 126px;
  left: 21px;
}
#rotary-table #start-btn {
  position: absolute;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
  top: 125px;
  left: 124px;
  width: 90px;
  height: 96px;
  line-height: 90px;
  text-align: center;
}

